{
  "name": "back-web-bde",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build && tsc -p test/tsconfig.json && tap --ts test/**/*.test.ts",
    "start": "fastify start dist/app.js",
    "build": "prisma generate && tsc",
    "dev": "tsc && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"tsc -w\" \"fastify start -p 4000 --ignore-watch=.ts$ -w -l info -P dist/app.js\""
  },
  "keywords": [],
  "author": "Alexandre Gomez",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^3.7.0",
    "bcrypt": "^5.0.1",
    "fastify": "^3.0.0",
    "fastify-autoload": "^3.3.1",
    "fastify-cli": "^2.14.0",
    "fastify-multipart": "^5.3.1",
    "fastify-plugin": "^3.0.0",
    "fastify-sensible": "^3.1.0",
    "fastify-swagger": "^4.13.1",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "minio": "^7.0.26",
    "nodemailer": "^6.7.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/form-data": "^2.5.0",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/minio": "^7.0.12",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.4",
    "@types/tap": "^15.0.0",
    "concurrently": "^6.0.0",
    "fastify-tsconfig": "^1.0.1",
    "prisma": "^3.7.0",
    "tap": "^15.0.9",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4"
  }
}
